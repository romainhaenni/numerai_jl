# Numerai Tournament System - Environment Configuration Template
# Copy this file to .env and fill in your actual API credentials

# Numerai API Credentials
# Get your credentials from https://numer.ai/account
NUMERAI_PUBLIC_ID=your_public_id_here
NUMERAI_SECRET_KEY=your_secret_key_here

# Optional: Tournament Configuration
# NUMERAI_TOURNAMENT_ID=8  # 8 for Classic, 11 for Signals

# Optional: Model Configuration
# NUMERAI_MODEL_NAME=your_model_name

# Optional: Directory Configuration
# NUMERAI_DATA_DIR=data
# NUMERAI_MODEL_DIR=models

# Optional: System Configuration
# JULIA_NUM_THREADS=16  # Number of threads for parallel processing
# JULIA_CPU_TARGET=native  # CPU optimization target